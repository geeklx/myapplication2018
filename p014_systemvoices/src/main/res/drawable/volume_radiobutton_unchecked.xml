<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center">
        <shape android:shape="oval">
            <solid
                android:color="#00000000" />
            <size
                android:width="40dp"
                android:height="40dp"/>
            <padding
                android:bottom="0dp"
                android:left="0dp"
                android:right="0dp"
                android:top="0dp"/>
        </shape>
    </item>
    <item android:gravity="center">
        <shape android:shape="oval">
            <stroke
                android:width="2dp"
                android:color="#6D6B66" />
            <size
                android:width="30dp"
                android:height="30dp"/>
            <padding
                android:bottom="0dp"
                android:left="0dp"
                android:right="0dp"
                android:top="0dp"/>
        </shape>
    </item>
